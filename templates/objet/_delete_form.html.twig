<form method="post" action="{{ path('app_objet_delete', {'id_object': objet.id_object}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet objet ?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ objet.id_object) }}">
    <button class="btn btn-danger">
        <i class="fas fa-trash"></i> Supprimer
    </button>
</form>
